{
  "expo": {
    "name": "AstroRide",
    "description": "Ride hailing app",
    "slug": "astro-ride-app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#652121"
    },
    "notification": {
      "icon": "./assets/images/notification.png"
    },
    "updates": {
      "enabled": true
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "android": {
      "package": "com.primetech.astro",
      "permissions": [
        "NOTIFICATIONS",
		"ACCESS_COARSE_LOCATION",
		"ACCESS_FINE_LOCATION"
      ],
	  "config": {
         "googleMaps": {
           "apiKey": "AIzaSyDq8YO8Juh__b4bhRCoSbDu3gPS407qjlM"
        }  
	  }
	  
    },
	"ios": {
	  "bundleIdentifier": "com.primetech.astro",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      },
	  "config": {
         "googleMapsApiKey": "AIzaSyDq8YO8Juh__b4bhRCoSbDu3gPS407qjlM" 
	  }
    }
  }
}
